<head>
	<title>Attendee Report</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body>
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
  <span class="navbar-toggler-icon"></span>
</button>
<div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="/login/landingPage">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/events">Events</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/tickets">My Tickets</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/login/logout">Logout</a>
      </li>
    </ul>
  </div>
</nav>
	<a class="btn btn-primary" href="/event/view/<%=event_id%>" role="button" style="margin:12px">Back</a>
	<div class="container">

	<% if(err && err.length > 0) { %>
	<div class="alert alert-danger" role="alert">
		Error in details provided: <%= err %>
	</div>
	<% } %>
	<% if (succ && succ.length > 0) { %>
	<div class="alert alert-success" role="alert">
		<%= succ %>
	</div>
	<% } %>
	<table class="table table-sm">
		<thead>
			<th>Name</th>
			<th>Email</th>
			<th>Access Requirements</th>
			<th>Dietary Requirements</th>
		</thead>
		<tbody>
			<% for(var i=0; i < attending.length; i++) { %>
			<tr>
				<td><%=names[i]%></td>
				<td><%=attending[i].email%></td>
				<td><%=attending[i].accessRequirements%></td>
				<td><%=attending[i].dietaryRestrictions%></td>
			</tr>
			<% } %>
		</tbody>
	</table>
	</div>
</body>
